/* 재귀함수를 사용하면 시간 오버로 오류뜸
#include <iostream>
using namespace std;

int factorial(int n){
    if(n==1){return 1;}
    else{
        return n*factorial(n-1);
    }
}

int main(){
    int n;
    cin>>n;
    
    cout<<factorial(n);
    return 0;
}*/

#include <iostream>
using namespace std;

int main(){
    int num = 1;
    int n;
    cin >> n;
    
    for(int i=1;i<=n;i++){
        num *= i;
    }
    
    cout << num;
    return 0;
}
