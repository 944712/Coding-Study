#include <iostream>

using namespace std;

int main(){
    //피보나치 수열의 n번째
    int n;
    //피보나치 수열 값을 저장할 변수
    int answer[30]={0,};
    
    answer[0]=0;
    answer[1]=1;
    cin >> n;

     if(n>=2){
         for(int i=2;i<=n;i++){
         //피보나치 수열 식:Fn = Fn-1 + Fn-2 (n>=2)
              answer[i]=answer[i-1]+answer[i-2];
         }
        }
        
       cout << answer[n] << endl;
    }

    
